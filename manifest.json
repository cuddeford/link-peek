{
  "name": "Link peek",
  "description": "Preview links without opening them like with iOS",
  "version": "1.0.0",
  "permissions": [],
  "background": {
    "scripts": ["sample.js"]
  },
  "content_scripts": [{
    "matches": ["https://*/*", "http://*/*"],
    "js": ["jquery.js", "inject.js"]
  }],
  "manifest_version": 2
}
